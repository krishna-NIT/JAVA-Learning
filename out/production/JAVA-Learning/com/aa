#include<iostream>
using namespace std;
int main()
{
 int max=0, min=0,arr[10000],n;
 cout<<"Enter the number of elements in list : ";
 cin>>n;
 cout<<"Enter the elements of list : "<<endl;
 for(int i=0; i<n;i++)
 {
 cin>>arr[i];
 }
 max=arr[0];
 min=arr[0];
 for(int i=1;i<n;i++)
 {
 if(arr[i]>max)
 {
 max=arr[i];
 }
 if(arr[i]<min)
 {
 min=arr[i];
 }
 }
 cout<<"The maximum element of the list is : "<< max<<endl;
 cout<<"The minimum element of the list is : "<< min<<endl;
}